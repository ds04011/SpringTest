<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.ds04011.SpringTest.bookmark.repository.BookmarkRepository">
 	<insert id="addBookmark"
 	parameterType="com.ds04011.SpringTest.bookmark.domain.Bookmark">
 		INSERT INTO `bookmark`
 		(
 			`name`
 			, `url`
 		)
 		VALUE
 		(
 			#{name}
 			, #{url}
 		)
 	
 	</insert>
 
 </mapper>
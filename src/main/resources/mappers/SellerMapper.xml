<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.ds04011.SpringTest.seller.repository.SellerRepository">
	
	<select>
	
	</select>
	
	<insert id="addSeller"
	parameterType="map">
		INSERT INTO 
			`seller`
		(
		`nickname`
		, `temperature`
		, `profileImage`
		)
		VALUE
		(
			#{nickname}
			, #{temperature}
			, #{url}
		)
	</insert>

</mapper> 